{
  "name": "microrr",
  "version": "1.0.0",
  "description": "microrr - Microservices Registry and Routing Framework",
  "main": "./dist/microrr.js",
  "bin": {
    "microrr": "./dist/microrr.js"
  },
  "scripts": {
    "start": "node dist/microrr.js",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "nyc mocha -r ts-node/register test/**/*.spec.ts",
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/erolms/microrr.git"
  },
  "keywords": [
    "microservices",
    "cloud",
    "registry",
    "router",
    "fabric",
    "mesh",
    "service mesh",
    "service registry",
    "fabric mesh",
    "router mesh",
    "internet of things",
    "iot"
  ],
  "author": "Erol Zavidic <erolms@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erolms/microrr/issues"
  },
  "homepage": "https://microrr.org/",
  "dependencies": {
    "commander": "2.15.0",
    "lodash": "4.17.5",
    "winston": "2.4.1"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/lodash": "4.14.105",
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.7",
    "@types/winston": "2.3.8",
    "chai": "4.1.2",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  }
}
